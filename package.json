        "eslint": "8.57.1",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-airbnb-typescript": "17.1.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "expect": "29.7.0",
        "mocha": "10.7.3",
        "mocha-expect-snapshot": "7.0.1",
        "rimraf": "5.0.0",
